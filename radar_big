#include <Servo.h>. 
const int trigPin = 8;
const int echoPin = 9;
int buz=7; 
// defining time and distance
long duration;
int distance;
Servo myServo; 

void setup() {
  pinMode(trigPin, OUTPUT); // trigPin as an Output
  pinMode(echoPin, INPUT); // echoPin as an Input
  pinMode(buz, OUTPUT); 
  Serial.begin(9600);
  myServo.attach(10); // Pin Connected To Servo
}